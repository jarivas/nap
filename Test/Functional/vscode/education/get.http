###
# @name login
POST http://nap.local HTTP/1.1

{
    "module": "user",
    "action": "login",
    "parameters": {
        "username": "{{username}}",
        "password": "{{password}}"
    }
}

###
@token = {{login.response.body.token}}

###
# @name getEducation
POST http://nap.local HTTP/1.1

{
    "module": "education",
    "action": "read",
    "parameters": {
        "token": "{{token}}"
    }
}
